{
  "name": "app-template",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "dev": "concurrently -k \"pnpm dev:server\" \"pnpm dev:metro\"",
    "dev:server": "cross-env NODE_ENV=development tsx watch --tsconfig tsconfig.server.json server/_core/index.ts",
    "dev:server:rust": "cd rust-server && ./target/release/protocol_guide_server",
    "dev:metro": "cross-env EXPO_USE_METRO_WORKSPACE_ROOT=1 npx expo start --web --port ${EXPO_PORT:-8081}",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:build": "docker compose build --no-cache",
    "docker:dev": "docker compose --profile dev up dev",
    "build": "esbuild server/_core/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "prebuild:web": "mkdir -p node_modules/react-native-css-interop/.cache && touch node_modules/react-native-css-interop/.cache/web.css",
    "build:web": "NODE_ENV=production npx expo export --platform web && node scripts/inject-pwa-meta.js && cp -r public/* dist/",
    "build:rust": "cd rust-server && cargo build --release",
    "start": "NODE_ENV=production node dist/index.js",
    "start:rust": "cd rust-server && ./target/release/protocol_guide_server",
    "check": "tsc --noEmit",
    "lint": "expo lint",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "vitest run && playwright test",
    "bench": "vitest bench",
    "bench:perf": "vitest run tests/performance/",
    "bench:report": "tsx scripts/run-benchmarks.ts",
    "bench:json": "tsx scripts/run-benchmarks.ts --json",
    "db:push": "drizzle-kit generate && drizzle-kit migrate",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "qr": "node scripts/generate_qr.mjs",
    "analyze": "npx expo export --platform web --output-dir dist-analyze && du -sh dist-analyze/_expo/static/js/web/*"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@expo/vector-icons": "^15.0.3",
    "@react-email/components": "^1.0.6",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^5.1.2",
    "@react-navigation/bottom-tabs": "^7.8.12",
    "@react-navigation/elements": "^2.9.2",
    "@sentry/node": "^10.36.0",
    "@supabase/supabase-js": "^2.49.4",
    "@tanstack/react-query": "^5.90.12",
    "@trpc/client": "^11.8.1",
    "@trpc/react-query": "^11.8.1",
    "@trpc/server": "^11.8.1",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "clsx": "^2.1.1",
    "cookie": "^1.1.1",
    "dotenv": "^16.6.1",
    "drizzle-orm": "^0.44.7",
    "expo": "~54.0.29",
    "expo-auth-session": "^7.0.10",
    "expo-build-properties": "^1.0.10",
    "expo-clipboard": "^8.0.8",
    "expo-constants": "~18.0.13",
    "expo-crypto": "^15.0.8",
    "expo-device": "~8.0.10",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "^15.0.8",
    "expo-linking": "~8.0.10",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.19",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-web-browser": "^15.0.10",
    "express": "^4.22.1",
    "helmet": "^8.1.0",
    "jose": "6.1.0",
    "mysql2": "^3.16.0",
    "nativewind": "^4.2.1",
    "pdf-parse": "^2.4.5",
    "pino": "^10.2.1",
    "pino-http": "^11.0.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^5.2.5",
    "react-native": "0.81.5",
    "react-native-css-interop": "^0.2.1",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.6",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "~0.21.2",
    "react-native-worklets": "0.5.1",
    "resend": "^6.8.0",
    "stripe": "^20.1.2",
    "superjson": "^1.13.3",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/cookie": "^0.6.0",
    "@types/express": "^4.17.25",
    "@types/node": "^22.19.3",
    "@types/pdf-parse": "^1.1.5",
    "@types/qrcode": "^1.5.6",
    "@types/react": "~19.1.17",
    "@vitest/coverage-v8": "^2.1.9",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.31.8",
    "esbuild": "^0.25.12",
    "eslint": "^9.39.2",
    "eslint-config-expo": "~10.0.0",
    "prettier": "^3.7.4",
    "qrcode": "^1.5.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vitest": "^2.1.9"
  },
  "pnpm": {
    "overrides": {
      "qs": ">=6.14.1",
      "tar": ">=7.5.4",
      "undici": ">=6.23.0",
      "esbuild": ">=0.25.0"
    }
  },
  "packageManager": "pnpm@9.12.0"
}
